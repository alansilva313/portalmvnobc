generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Bloqueio {
  id          String   @id @default(uuid())
  documento   String
  birthday    DateTime     @db.Date
  tipo_bloqueio String
  linha String
  created_at  DateTime @default(now())

  @@map("bloqueio")
}

